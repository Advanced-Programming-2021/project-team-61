<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="850.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ChangeNickName">
   <children>
      <ImageView fitHeight="850.0" fitWidth="1300.0">
         <image>
            <Image url="@../startPage.png" />
         </image>
      </ImageView>
      <TextField fx:id="nickName" layoutX="488.0" layoutY="617.0" promptText="new nickName :">
         <font>
            <Font size="25.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </TextField>
      <Button fx:id="set" layoutX="736.0" layoutY="718.0" mnemonicParsing="false" onAction="#set" text="set">
         <font>
            <Font name="Eras Light ITC" size="25.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
      <Button fx:id="back" layoutX="488.0" layoutY="718.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font name="Eras Light ITC" size="25.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
   </children>
</AnchorPane>
