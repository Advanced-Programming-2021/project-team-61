<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Sphere?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<ScrollPane pannable="true" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.CreateNewGamePage">
   <content>
      <AnchorPane prefHeight="850.0" prefWidth="1300.0">
         <children>
            <ImageView fitHeight="850.0" fitWidth="1387.0">
               <image>
                  <Image url="@../createNewGame.png" />
               </image>
            </ImageView>
            <Sphere layoutX="477.0" layoutY="723.0" radius="50" />
            <Text fx:id="round" fill="WHITE" layoutX="321.0" layoutY="735.0" strokeType="OUTSIDE" strokeWidth="0.0" text="round">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Text>
            <Text fx:id="numOfRound" layoutX="470.0" layoutY="733.0" strokeType="OUTSIDE" strokeWidth="0.0" text="1">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <Button fx:id="positive" layoutX="540.0" layoutY="691.0" mnemonicParsing="false" onAction="#positive" text="+" />
            <Button fx:id="negative" layoutX="540.0" layoutY="724.0" mnemonicParsing="false" onAction="#negative" prefHeight="19.0" prefWidth="31.0" text="-">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <TextField fx:id="username" layoutX="796.0" layoutY="697.0" promptText="rival username :">
               <font>
                  <Font size="25.0" />
               </font>
            </TextField>
            <Button fx:id="start" layoutX="642.0" layoutY="591.0" mnemonicParsing="false" onAction="#start" text="START">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="back" layoutX="89.0" layoutY="773.0" mnemonicParsing="false" onAction="#back" text="back" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
