<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="850.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ChangePassword">
   <children>
      <ImageView fitHeight="850.0" fitWidth="1300.0">
         <image>
            <Image url="@../startPage.png" />
         </image>
      </ImageView>
      <TextField fx:id="oldPass" layoutX="526.0" layoutY="578.0" promptText="last password :">
         <font>
            <Font size="20.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </TextField>
      <TextField fx:id="newPass" layoutX="526.0" layoutY="638.0" promptText="new password :">
         <font>
            <Font size="20.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </TextField>
      <Button fx:id="check" layoutX="712.0" layoutY="727.0" mnemonicParsing="false" onAction="#check" text="set">
         <font>
            <Font name="Eras Light ITC" size="25.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
      <Button fx:id="back" layoutX="526.0" layoutY="727.0" mnemonicParsing="false" onAction="#back" text="back">
         <font>
            <Font name="Eras Light ITC" size="25.0" />
         </font>
         <effect>
            <SepiaTone />
         </effect>
      </Button>
   </children>
</AnchorPane>
