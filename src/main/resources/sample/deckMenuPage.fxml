<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="850.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.DeckMenuPage">
   <children>
      <TextField fx:id="deckName" alignment="CENTER" layoutX="526.0" layoutY="716.0" promptText="deck name:">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="removeDeck" layoutX="375.0" layoutY="716.0" mnemonicParsing="false" onAction="#removeDeck" text="remove deck">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="createDeck" layoutX="526.0" layoutY="664.0" mnemonicParsing="false" onAction="#createDeck" prefHeight="44.0" prefWidth="249.0" text="create new deck">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="activate" layoutX="527.0" layoutY="768.0" mnemonicParsing="false" onAction="#activate" prefHeight="44.0" prefWidth="249.0" text="set as activate deck">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="edit" layoutX="786.0" layoutY="716.0" mnemonicParsing="false" onAction="#edit" text="edit deck">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="44.0" layoutY="784.0" mnemonicParsing="false" onAction="#back" text="back" />
   </children>
</AnchorPane>
